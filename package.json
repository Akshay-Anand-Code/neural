{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "clean": "rm -rf dist node_modules package-lock.json docs/dist docs/node_modules docs/.vitepress/dist docs/.vitepress/cache .vite",
    "docs:dev": "cd docs && npm install && vitepress dev",
    "docs:build": "cd docs && npm install && vitepress build && cp -r dist ../dist/docs",
    "docs:preview": "cd docs && npm install && vitepress preview",
    "start": "vite preview",
    "deploy": "node scripts/deploy.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "cd docs && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "papaparse": "^5.4.1",
    "@react-spring/three": "^9.7.3",
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.12",
    "@types/three": "^0.172.0",
    "openai": "^4.28.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.1",
    "react-use-websocket": "^4.5.0",
    "three": "^0.161.0",
    "framer-motion": "^12.0.6",
    "lucide-react": "^0.474.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.12.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "terser": "^5.29.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitepress": "^1.0.0-rc.44"
  }
}